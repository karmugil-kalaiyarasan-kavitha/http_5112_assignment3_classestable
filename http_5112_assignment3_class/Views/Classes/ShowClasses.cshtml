@model IEnumerable<http_5112_assignment3_class.Models.Classes>

@{
    ViewBag.Title = "ShowClasses";
}

<a href="/Classes/List">Back to List</a>

@foreach (var Classes in Model)
{
    <div>
        <table>
            <tr>
                <th>Class ID</th>
                <th>Teacher ID</th>
                <th>Class Code</th>
                <th>Start Date</th>
                <th>Finish Date</th>
                <th>Class Name</th>
            </tr>
            <tr>
                <td>@Classes.classId</td>
                <td>@Classes.teacherId</td>
                <td>@Classes.classCode</td>
                <td>@Classes.startDate</td>
                <td>@Classes.finishDate</td>
                <td>@Classes.className</td>
            </tr>
        </table>

    </div>
}
